<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neuralgalaxy.stars.users.dao.mapper.UserMapper">

    <select id="selectByName"  resultType="com.neuralgalaxy.stars.users.dao.entity.UserEntity">
        SELECT *
        FROM Users
        WHERE username = #{username}
    </select>

</mapper>
